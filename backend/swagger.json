    "/users": {
      "post": {
        "tags": ["User"],
        "summary": "Create a new user",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  },
                  "full_name": {
                    "type": "string"
                  },
                  "social_name": {
                    "type": "string"
                  },
                  "cpf": {
                    "type": "string"
                  },
                  "sex": {
                    "enum": ["M", "F"]
                  },
                  "date_of_birth": {
                    "type": "string"
                  },
                  "role": {
                    "enum": ["NORMAL", "THEATER_ADMIN", "MOVIE_CURATOR", "ADMIN"]
                  },
                  "cellphone": {
                    "type": "object",
                    "properties": {
                      "ddd": {
                        "type": "string"
                      },
                      "number": {
                        "type": "string"
                      }
                    }
                  },
                  "state_id": {
                    "type": "string"
                  },
                  "city_id": {
                    "type": "string"
                  }
                },
                "example": {
                  "email": "string",
                  "password": "string",
                  "full_name": "string",
                  "social_name": "string",
                  "cpf": "string",
                  "sex": "M",
                  "date_of_birth": "2023-12-14T00:00:00.000Z",
                  "cellphone": {
                    "ddd": "21",
                    "number": "911112222"
                  },
                  "state_id": "string",
                  "city_id": "string"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          },
          "409": {
            "description": "Email already exists",
