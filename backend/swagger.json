  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat":"JWT"
      }
    },
    "schemas": {
      "MovieRequest": {
        "type": "object",
        "properties": {
          "tmdb_id": {
            "type": "number"
          },
          "name": {
            "type": "string"
          },
          "original_name": {
            "type": "string"
          },
          "synopsis": {
            "type": "string"
          },
          "genres": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "number"
                }
              },
              "required": ["id"]
            }
          },
          "duration": {
            "type": "number"
          },
          "release_date": {
            "type": "string"
          },
          "poster_path": {
            "type": "string"
          },
          "max_date": {
            "type": "string"
          },
          "directors": {
            "type": "string"
          },
          "quantity_avaiable": {
            "type": "number"
          }
        },
        "required": ["tmdb_id", "name", "original_name", "synopsis", "genres", "duration", "release_date", "poster_path", "max_date", "quantity_avaiable"],
        "example": {
          "tmdb_id": 0,
          "name": "string",
          "original_name": "string",
          "synopsis": "string",
          "genres": [
            { "id": 0 }
          ],
          "duration": 0,
          "release_date": "2023-12-14T00:00:00.000Z",
